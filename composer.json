{
    "name": "onvardgmbh/wp-formendpoint",
    "description": "A endpoint for forms in Wordpress themes.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bj√∂rn Hasse",
            "email": "hasse@onvard.de"
        }
    ],
    "autoload": {
        "psr-4": {
          "Onvardgmbh\\Formendpoint\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.7",
        "phpstan/phpstan": "^0.8.5",
        "phpunit/phpunit": "^7.5",
        "vlucas/phpdotenv": "^3.3"
    },
    "scripts": {
        "php-cs": "php-cs-fixer fix",
        "php-cs-dry": "php-cs-fixer fix --dry-run",
        "phpstan": "phpstan analyze -c phpstan.neon src",
        "test": "phpunit"
    }
}
